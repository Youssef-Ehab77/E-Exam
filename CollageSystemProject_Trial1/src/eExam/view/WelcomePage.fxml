<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.Glow?>
<?import javafx.scene.effect.SepiaTone?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="800.0" prefWidth="1100.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="eExam.controller.Welcome_Controller">
    <ImageView fitHeight="802.0" fitWidth="1103.0" layoutY="1.0" opacity="0.93" pickOnBounds="true">
        <image>
            <Image url="@Images/test1.jpg" />
        </image>
    </ImageView>
    <Button fx:id="btn_professor" layoutX="469.0" layoutY="310.0" mnemonicParsing="false" onAction="#press_professor" text="Professor">
        <font>
            <Font size="33.0" />
        </font>
        <effect>
            <SepiaTone level="0.2" />
        </effect>
        <cursor>
            <Cursor fx:constant="HAND" />
        </cursor>
    </Button>
    <Button fx:id="btn_Student" layoutX="469.0" layoutY="403.0" mnemonicParsing="false" onAction="#press_student" prefHeight="71.0" prefWidth="180.0" text="Student">
        <font>
            <Font size="33.0" />
        </font>
        <effect>
            <Glow />
        </effect>
    </Button>
    <Label alignment="CENTER" layoutX="251.0" layoutY="54.0" prefHeight="181.0" prefWidth="609.0" text="College System " textAlignment="CENTER" textFill="WHITE">
        <font>
            <Font name="System Bold" size="72.0" />
        </font>
    </Label>
</AnchorPane>
