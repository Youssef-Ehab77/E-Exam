<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="750.0" prefWidth="750.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="eExam.controller.Register_Controller">
    <Label alignment="CENTER" layoutX="229.0" layoutY="103.0" prefHeight="65.0" prefWidth="292.0" text="Welcome to register">
        <font>
            <Font size="29.0" />
        </font>
    </Label>
    <PasswordField fx:id="pf_confirm_password" layoutX="260.0" layoutY="373.0" prefHeight="30.0" prefWidth="250.0" promptText="Confirm Password" />
    <TextField fx:id="tf_username" layoutX="260.0" layoutY="261.0" prefHeight="30.0" prefWidth="250.0" promptText="Username" />
    <PasswordField fx:id="pf_password" layoutX="260.0" layoutY="317.0" prefHeight="30.0" prefWidth="250.0" promptText="Password" />
    <Button fx:id="btn_signup" layoutX="345.0" layoutY="427.0" mnemonicParsing="false" onAction="#press_signup" prefHeight="41.0" prefWidth="80.0" text="SignUp">
        <font>
            <Font size="15.0" />
        </font>
    </Button>
   <Label fx:id="lbl_error" alignment="CENTER" layoutX="201.0" layoutY="192.0" prefHeight="30.0" prefWidth="368.0" textAlignment="CENTER" textFill="#f80808">
      <font>
         <Font size="17.0" />
      </font>
   </Label>
   <Button fx:id="btn_back_to_login" layoutX="521.0" layoutY="573.0" mnemonicParsing="false" onAction="#press_back_to_login" prefHeight="42.0" prefWidth="161.0" text="Back To Login Page">
      <font>
         <Font size="15.0" />
      </font>
   </Button>
</AnchorPane>
